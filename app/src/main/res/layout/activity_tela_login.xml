<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TelaLogin">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="434dp"
        android:layout_height="851dp"
        android:src="@drawable/blurbackground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnEnviar4"
        android:layout_width="122dp"
        android:layout_height="33dp"
        android:layout_marginStart="220dp"
        android:layout_marginBottom="348dp"
        android:background="@drawable/cancela"
        android:text="Enviar"
        android:textAllCaps="false"
        android:textColor="#fff"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnEnviar3"
        android:layout_width="122dp"
        android:layout_height="33dp"
        android:layout_marginStart="19dp"
        android:layout_marginEnd="212dp"
        android:layout_marginBottom="348dp"
        android:background="@drawable/envia"
        android:text="Cancelar"
        android:textAllCaps="false"
        android:textColor="#fff"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnEnviar" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="183dp"
        android:layout_height="49dp"
        android:layout_marginTop="44dp"
        android:fontFamily="sans-serif-black"
        android:text="Bem-vindo"
        android:textAllCaps="false"
        android:textSize="34dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.625" />
</androidx.constraintlayout.widget.ConstraintLayout>